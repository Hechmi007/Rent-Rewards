
   <div class="container">
    {{ form_start(form , {'attr':{'novalidate':'novalidate'}}) }}
    <div class="form-group">
        {{ form_label(form.numcarte,"Numero de carte")  }}
        {{ form_widget(form.numcarte , {'attr':{'class':'form-control'} }) }}
        {% if form_errors(form.numcarte) is not empty %}
            <div class="alert alert-danger"> 
                {{ form_errors(form.numcarte) }}
            </div>
        {% endif %}
    </div>
    <div class="form-group">
        {{ form_label(form.datedebut,"Date debut")  }}
        {{ form_widget(form.datedebut , {'attr':{'class':'form-control'} }) }}
        <div class="alert alert-danger"> 
            {{ form_errors(form.datedebut) }}
        </div>
    </div>
    <div class="form-group">
        {{ form_label(form.datefin,"Date Fin")  }}
        {{ form_widget(form.datefin , {'attr':{'class':'form-control'} }) }}
        <div class="alert alert-danger"> 
            {{ form_errors(form.datefin) }}
        </div>
    </div>
    <div class="form-group">
        {{ form_label(form.username,"user name")  }}
        {{ form_widget(form.username , {'attr':{'class':'form-control'} }) }}
        {% if form_errors(form.username) is not empty %}
            <div class="alert alert-danger"> 
                {{ form_errors(form.username) }}
            </div>
        {% endif %}
    </div>
    <button class="btn btn-primary text-white py-3 px-4"  class="btn">{{ Button_label|default('Save') }}</button>
    {{ form_end(form) }}
        
    </di>